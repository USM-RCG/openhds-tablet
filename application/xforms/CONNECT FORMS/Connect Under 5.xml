      <instance>
        <connect_under_5 id="connect_under_5">
          <start/>
          <deviceid/>
          <today/>
          <fieldWorkerId/>
          <individId/>
          <firstName/>
          <middleName/>
          <lastName/>
          <dob/>
          <feverLast2Wks/>
          <mostRecentAffliction/>
          <seekTreatment/>
          <soughtTreatmentWhere/>
          <hadHomeVisit/>
          <whoVisited/>
          <whoVisitedOther/>
          <meta>
            <instanceID/>
          </meta>
        </connect_under_5>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/connect_under_5/start" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/connect_under_5/deviceid" type="string"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/connect_under_5/today" type="date"/>
      <bind nodeset="/connect_under_5/fieldWorkerId" type="string"/>
      <bind nodeset="/connect_under_5/individId" required="true()" type="string"/>
      <bind nodeset="/connect_under_5/firstName" required="true()" type="string"/>
      <bind nodeset="/connect_under_5/middleName" required="true()" type="string"/>
      <bind nodeset="/connect_under_5/lastName" required="true()" type="string"/>
      <bind constraint=".&lt;=/connect_under_5/today" nodeset="/connect_under_5/dob" required="true()" type="date"/>
      <bind nodeset="/connect_under_5/feverLast2Wks" required="true()" type="select1"/>
      <bind nodeset="/connect_under_5/mostRecentAffliction" required="true()" type="select1"/>
      <bind nodeset="/connect_under_5/seekTreatment" required="true()" type="select1"/>
      <bind nodeset="/connect_under_5/soughtTreatmentWhere" required="true()" type="select1"/>
      <bind nodeset="/connect_under_5/hadHomeVisit" required="true()" type="select1"/>
      <bind nodeset="/connect_under_5/whoVisited" relevant="selected(/connect_under_5/hadHomeVisit,'1')" required="true()" type="select1"/>
      <bind nodeset="/connect_under_5/whoVisitedOther" relevant="selected(/connect_under_5/whoVisited,'2')" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/connect_under_5/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
