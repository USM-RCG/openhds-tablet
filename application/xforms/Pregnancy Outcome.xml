			<bind nodeset="/data/outcomes/outcomeType" type="select1" required="true()" />
			<bind nodeset="/data/outcomes/childId" readonly="true()" relevant="/data/outcomes/outcomeType = 'LBR'" type="string"
					required="true()" />
			<bind nodeset="/data/outcomes/firstName" relevant="/data/outcomes/outcomeType = 'LBR'" type="string"
					required="true()" />
			<bind nodeset="/data/outcomes/middleName" relevant="/data/outcomes/outcomeType = 'LBR'" type="string"
					required="true()" />
			<bind nodeset="/data/outcomes/lastName" relevant="/data/outcomes/outcomeType = 'LBR'" type="string"
					required="true()" />
			<bind nodeset="/data/outcomes/gender" relevant="/data/outcomes/outcomeType = 'LBR'" type="select1"
					required="true()" />
			<bind nodeset="/data/partialDate" type="select1"
					required="true()" />
			<bind nodeset="/data/outcomes/socialGroupId" readonly="true()" relevant="/data/outcomes/outcomeType = 'LBR'" type="string"
					required="true()" />
			<bind nodeset="/data/outcomes/relationshipToGroupHead" required="true()" relevant="/data/outcomes/outcomeType = 'LBR'"
					type="select1" />
			<bind nodeset="/data/outcomes/ordinalPosition" required="true()" relevant="/data/outcomes/outcomeType = 'LBR'"
					type="int" />
			<bind nodeset="/data/birthingPlace" required="true()" type="select1" />
			<bind nodeset="/data/birthingPlaceOther" required="true()" relevant="../birthingPlace='OTHER'" type="string" />
			<bind nodeset="/data/hoursOrDaysInHospital" required="true()" relevant="../birthingPlace='HOSPITAL'" type="select1" />
			<bind nodeset="/data/hoursInHospital" required="true()" relevant="../hoursOrDaysInHospital='HOURS'" type="int" />
			<bind nodeset="/data/daysInHospital" required="true()" relevant="../hoursOrDaysInHospital='DAYS'" type="int" />
			<bind nodeset="/data/hoursOrDaysBefore1stPostnatalVisit" required="true()" relevant="../birthingPlace='HOME' or ../birthingPlace='OTHER'" type="select1" />
			<bind nodeset="/data/hoursBefore1stPostnatalVisit" required="true()" relevant="../hoursOrDaysBefore1stPostnatalVisit='HOURS'"  type="int" />
			<bind nodeset="/data/daysBefore1stPostnatalVisit" required="true()" relevant="../hoursOrDaysBefore1stPostnatalVisit='DAYS'" type="int" />
			<bind nodeset="/data/birthingAssistant" required="true()" type="select1" />
			<bind nodeset="/data/birthingAssistantOther" required="true()" relevant="../birthingAssistant='OTHER'" type="string" />
			<bind nodeset="/data/caesarianOrNatural" required="true()" type="select1" />
			<bind nodeset="/data/totalNumberChildrenStillLiving" required="true()" type="int" />
			<bind nodeset="/data/attendedANC" required="true()" type="select1" />
			<bind nodeset="/data/numberOfANCAttendances" required="true()" relevant="../attendedANC = 'YES'" type="int" />


			<bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>                                       
		</model>
	</h:head>
	<h:body>
		<input ref="/data/motherId">
