					jr:constraintMsg="Expected delivery date cannot be before today."  />-->
				<bind nodeset="/data/expectedDeliveryDate" type="date" required="true()" constraint=". &gt;= today()"	calculate="if(../pregNotes = 3,/data/lastPeriodInterviewee,/data/lastPeriodFromPregNotes)"/>
			<bind nodeset="/data/estimatedAgeOfPreg" constraint=". &lt;=9" jr:constraintMsg="Miezi haiwezi kuzidi 9" type="int" required="true()" />
