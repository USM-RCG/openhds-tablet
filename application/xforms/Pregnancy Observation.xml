			<bind nodeset="/data/lastPeriodInterviewee" relevant="../pregNotes = 3" type="date" constraint=". &lt;= today()" required="true()" />
			<bind nodeset="/data/lastPeriodFromPregNotes" relevant="../pregNotes = 1 or ../pregNotes = 2" type="date" constraint=". &lt;= today()" required="true()" />
			<bind nodeset="/data/hasANCClinicVisit" type="select1" required="true()" />
			<bind nodeset="/data/lastClinicVisitDate" relevant ="../hasANCClinicVisit=1" type="date" constraint=". &lt;= today()" required="true()" />
			<bind nodeset="/data/pregnancyNumber" type="int" required="true()" />
			<bind nodeset="/data/haveMosquitoNet" type="select1" required="true()" />

			<bind nodeset="/data/sleptUnderMosquitoNet" relevant ="../haveMosquitoNet=1" type="select1" required="true()" />
			<bind nodeset="/data/treatedNet" relevant ="../haveMosquitoNet=1" type="select1" required="true()" />
			<bind nodeset="/data/monthsOrYears" relevant ="../treatedNet=1" type="select1" required="true()" />
			<bind nodeset="/data/monthsSinceLastNetTreatment" relevant ="../monthsOrYears=1" type="int" required="true()" />
			<bind nodeset="/data/yearsSinceLastNetTreatment" relevant ="../monthsOrYears=2" type="int" required="true()" />
			<bind nodeset="/data/sourceOfNet" relevant ="../haveMosquitoNet=1" type="select1" required="true()" />	
			<bind nodeset="/data/relationshipOfRespondentToExpectantMother" type="select1" required="true()" />			
			
			<bind nodeset="/data/recordedDate" type="date" readonly="true()" required="true()" />
