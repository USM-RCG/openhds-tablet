            <bind nodeset="/data/nameOfDestination"  type="string" required="true()" />
            <bind nodeset="/data/reasonForOutMigration" type="select1" required="true()" />
			<bind nodeset="/data/reasonForOutMigrationOther" relevant="../reasonForOutMigration = 'OTHER'" type="string" required="true()" />
